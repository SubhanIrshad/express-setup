{
  "name": "express",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "setup": "mkdir ./bin && mkdir ./bin/prod && mkdir ./bin/prod/client && mkdir ./bin/prod/src",
    "start": "npm run clean && babel src -d ./bin/prod/src --source-maps inline && clear && npm run serve",
    "clean": "rm -r ./bin/prod/src",
    "reset": "rm -r ./bin",
    "dev": "nodemon --exec babel-node src/index.js",
    "build": "npm run clean && babel src -d ./bin/prod/src",
    "prod": "npm run reset && npm run setup && npm run build && npm run start",
    "serve": "node ./bin/prod/src/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.3.1"
  }
}
